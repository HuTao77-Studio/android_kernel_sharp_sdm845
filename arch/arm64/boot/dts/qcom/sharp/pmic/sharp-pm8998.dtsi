#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>

/*****************************************/
/*  pm8998: qcom,pm8998 alias            */
/*****************************************/

&spmi_bus {
	qcom,pm8998@0 {
		qcom,power-on@800 {
			qcom,pon_2 {
				linux,code = <115>;
			};
			qcom,pon_3 {
				qcom,pon-type = <3>;
				qcom,support-reset = <0>;
				qcom,pull-up = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <2000>;
			};
		};
	};

	qcom,pm8998@1 {
	};
};

