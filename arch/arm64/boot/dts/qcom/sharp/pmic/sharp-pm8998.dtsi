/*****************************************/
/*  pm8998: rpm-regulator              */
/*****************************************/
&soc {
	rpmh-regulator-ldoa10 {
		qcom,send-defaults;
		pm8998_l10: regulator-l10 {	/* SIM2 */
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			qcom,init-voltage = <1800000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_LPM>;
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-ldoa14 {
		qcom,send-defaults;
		pm8998_l14: regulator-l14 {	/* LCD driver digital */
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			qcom,init-voltage = <1800000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_HPM>;
			qcom,init-enable = <1>;
		};
	};

	rpmh-regulator-ldoa18 {
		qcom,send-defaults;
		pm8998_l18: regulator-l18 {	/* DTV LNA */
			regulator-min-microvolt = <2856000>;
			regulator-max-microvolt = <2856000>;
			qcom,init-voltage = <2856000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_HPM>;
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-ldoa19 {
		qcom,send-defaults;
		pm8998_l19: regulator-l19 {	/* Proximity */
			regulator-min-microvolt = <2952000>;
			regulator-max-microvolt = <2952000>;
			qcom,init-voltage = <2952000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_LPM>;
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-ldoa21 {
		qcom,send-defaults;
		pm8998_l21: regulator-l21 {	/* Camera AF driver */
			regulator-min-microvolt = <2952000>;
			regulator-max-microvolt = <2952000>;
			qcom,init-voltage = <2952000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_HPM>;
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-ldoa22 {
//		qcom,use-awake-state;
		qcom,send-defaults;
		pm8998_l22: regulator-l22 {	/* Fingerprint */
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			qcom,init-voltage = <1800000>;
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-ldoa28 {
		qcom,send-defaults;
		pm8998_l28: regulator-l28 {	/* Touch panel analog */
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3000000>;
			qcom,init-voltage = <3000000>;
			qcom,init-mode = <RPMH_REGULATOR_MODE_HPM>;
			/delete-property/ qcom,init-enable;
		};
	};

	rpmh-regulator-vsa1 {
		qcom,send-defaults;
		pm8998_lvs1: regulator-lvs1 {	/* Camera IO */
			qcom,init-enable = <0>;
		};
	};

	rpmh-regulator-vsa2 {
		qcom,send-defaults;
		pm8998_lvs2: regulator-lvs2 {	/* Touch panel digital and /IO */
			/delete-property/ qcom,init-enable;
		};
	};
};
/*****************************************/
/*  qpnp-adc-voltage                     */
/*****************************************/
&pm8998_vadc {
	chan@4c { /* xo_therm */
		label = "xo_therm";
		reg = <0x4c>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <4>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@4d { /* msm_therm */
		label = "msm_therm";
		reg = <0x4d>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@4e { /* usb_therm */
		label = "usb_therm";
		reg = <0x4e>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@4f { /* pa_therm1 */
		label = "pa_therm1";
		reg = <0x4f>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@51 { /* lcd_therm */
		label = "lcd_therm";
		reg = <0x51>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@55 { /* cam_therm */
		label = "cam_therm";
		reg = <0x55>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};
};

/*****************************************/
/*  qpnp-vadc-tm                         */
/*****************************************/
&pm8998_adc_tm  {
	chan@4c { /* xo_therm */
		label = "xo_therm";
		reg = <0x4c>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <4>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x68>;	/* M1_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};

	chan@4d { /* msm_therm */
		label = "msm_therm";
		reg = <0x4d>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x70>;	/* M2_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};

	chan@4e { /* usb_therm */
		label = "usb_therm";
		reg = <0x4e>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x88>;	/* M5_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};

	chan@4f { /* pa_therm1 */
		label = "pa_therm1";
		reg = <0x4f>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x78>;	/* M3_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};

	chan@51 { /* quiet_therm -> lcd_therm */
		label = "lcd_therm";
		reg = <0x51>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x80>;	/* M4_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};

	chan@55 { /* cam_therm */
		label = "cam_therm";
		reg = <0x55>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,btm-channel-number = <0x98>;	/* M7_ADC_CH_SEL_CTL */
		qcom,thermal-node;
	};
};

/*****************************************/
/*  thermal_zones                        */
/*****************************************/
&thermal_zones {
	usb-therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm8998_adc_tm 0x4e>;
		thermal-governor = "user_space";

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	lcd-therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm8998_adc_tm 0x51>;
		thermal-governor = "user_space";

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	cam-therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm8998_adc_tm 0x55>;
		thermal-governor = "user_space";

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};

/*****************************************/
/*  SHARP Device Drivers                 */
/*****************************************/
&soc {
	/*---------------------------------------*/
	/*  Battery Control Driver               */
	/*---------------------------------------*/
	sharp,shbatt {
		compatible = "sharp,shbatt";
		qcom,shbatt-pm-vadc = <&pm8998_vadc>;
		sharp,cl-high-thresh = <45>;
		sharp,cl-low-thresh = <45>;
		sharp,pa_temp_label = "pa_therm1";
		status = "ok";
	};
};
